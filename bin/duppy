#!/usr/bin/env perl 
# PODNAME: duppy - a wrapper for configurations around casperjs
use strict;
use warnings;
use FindBin qw/$Bin/;
use lib "$Bin/../lib";
use App::Duppy;

my $duppy = App::Duppy->new_with_options();
$duppy->run_casper();

__END__

